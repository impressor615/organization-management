{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "declaration": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "baseUrl": "./",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "paths":{
        "@/*": ["src/*"]
      }
  },
  "include": [
      "./src/**/*",
      "./src/index.d.ts"
  ]
}